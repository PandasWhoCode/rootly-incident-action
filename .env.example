# dotenv-linter:off IncorrectDelimiter

# Do not commit your actual .env file to Git! This may contain secrets or other
# private information.

# Enable/disable step debug logging (default: `false`). For local debugging, it
# may be useful to set it to `true`.
ACTIONS_STEP_DEBUG=true

# GitHub Actions inputs should follow `INPUT_<name>` format (case-sensitive).
# Hyphens should not be converted to underscores!
INPUT_ALERT_EXTERNAL_ID=my-alert-external-id
INPUT_ALERT_EXTERNAL_URL=https://my-url-example.example
INPUT_ALERT_GROUPS="My Rootly Alert Group,My Other Rootly Alert Group"
INPUT_ALERT_SERVICE="My Rootly Alert Service"
INPUT_ALERT_URGENCY=High
INPUT_CREATE_ALERT=true
INPUT_CREATE_INCIDENT=true
INPUT_CREATE_PUBLIC_INCIDENT=false
INPUT_ENVIRONMENTS="My Rootly Environment"
INPUT_INCIDENT_TYPES="My Rootly Incident Type,My Other Rootly Incident Type"
INPUT_SERVICES="My Rootly Service"
INPUT_SEVERITY="My Rootly Severity"
INPUT_SUMMARY="My Incident Summary"
INPUT_TEAMS="My Rootly Team,My Other Rootly Team"
INPUT_TITLE="My Incident Title"

# GitHub Actions secrets should follow `INPUT_<name>` format (case-sensitive).
# Hyphens should not be converted to underscores!
INPUT_API_KEY=my-rootly-api-key

