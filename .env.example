# dotenv-linter:off IncorrectDelimiter

# Do not commit your actual .env file to Git! This may contain secrets or other
# private information.

# Enable/disable step debug logging (default: `false`). For local debugging, it
# may be useful to set it to `true`.
ACTIONS_STEP_DEBUG=true

# GitHub Actions inputs should follow `INPUT_<name>` format (case-sensitive).
# Hyphens should not be converted to underscores!
INPUT_ALERT_IDS="12345678-90ab-cdef-12345678, 23456789-0abc-def1-23456789"
INPUT_CAUSES="Test Cause, Another Cause"
INPUT_CREATE_PUBLIC_INCIDENT=false
INPUT_DUPLICATE_INCIDE_ID=12345678-90ab-cdef-12345678
INPUT_ENVIRONMENTS="Test Environment, Another Environment"
INPUT_FUNCTIONALITIES="Test Functionality, Another Functionality"
INPUT_GOOGLE_DRIVE_PARENT_ID=1A2B3C4D5E6F7G8H9I0J
INPUT_GOOGLE_DRIVE_URL=https://drive.google.com/drive/folders/1A2B3C4D5E6F7G8H9I0J
INPUT_INCIDENT_TYPES="Test Incident Type, Another Incident Type"
INPUT_KIND=test
INPUT_LABELS="Test Label: Value, Another Label: Another Value"
INPUT_NOTIFY_EMAILS="an.email@domain.com, another.email@domain.com"
INPUT_PARENT_INCIDENT_ID=12345678-90ab-cdef-12345678
INPUT_SERVICES="Test Service, Another Service"
INPUT_SEVERITY="Triage Event"
INPUT_SLACK_CHANNEL_ID=C12345678
INPUT_SLACK_CHANNEL_NAME="#test-channel"
INPUT_SLACK_CHANNEL_URL=https://your-workspace.slack.com/archives/C12345678
INPUT_SUMMARY="This is a test incident created from rootly-incident-action"
INPUT_TEAMS="Test Team, Another Team"
INPUT_TITLE="Test Incident from rootly-incident-action"
INPUT_URL="Default URL for Rootly Incidents"
INPUT_USER_EMAIL=an.email@domain.com

# GitHub Actions secrets should follow `INPUT_<name>` format (case-sensitive).
# Hyphens should not be converted to underscores!
INPUT_API_KEY=my-rootly-api-key

